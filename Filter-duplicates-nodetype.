filterDupl:
    derived_from: workflow_service
    properties:
      Normalize_Sequence_Names:
        type: boolean
        default: false
      Normalize_Duplicates_Only:
        type: boolean
        default: false
      #input
      fasta-files:
        type: string
        default: file-wrapper
      #outputs:
      filtered-fasta-files:
        type: string
        default: file-wrapper
      removed-sequences:
        type: string
        default: data-wrapper
      sequence-map:
        type: string
        default: data-wrapper
